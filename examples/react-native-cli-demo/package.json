{
  "name": "reactnativeqa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "cd ./ios && pod install && cd ../ && react-native run-ios",
    "clear:cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "start": "react-native link && react-native start",
    "create:android:release": "npm run build:android && react-native run-android --variant=release && mv ./android/app/build/outputs/apk/release/app-release.apk ./",
    "create:android:debug": "npm run build:android && react-native run-android --variant=debug && mv ./android/app/build/outputs/apk/debug/app-debug.apk ./",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "log:android": "react-native log-android",
    "lint": "eslint .",
    "haul": "haul start"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.6",
    "@flagship.io/react-native-sdk": "^2.1.6-alpha.1",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/cli-platform-android": "^7.0.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@types/react": "^16.9.21",
    "events": "^3.3.0",
    "native-base": "^3.4.1",
    "react-native": "^0.68.1",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-json-tree": "^1.3.0",
    "react-native-local-notification": "^3.0.12",
    "react-native-network-logger": "^1.12.0",
    "react-native-reanimated": "^2.7.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-style-tachyons": "^4.2.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "peerDependencies": {
    "react": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@haul-bundler/babel-preset-react-native": "^0.16.0",
    "@haul-bundler/preset-0.60": "^0.18.1",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.70.1",
    "prettier": "^2.0.4",
    "prettier-eslint": "^9.0.1",
    "react-native-elements": "^1.2.7",
    "react-test-renderer": "16.11.0",
    "redux-tools": "^3.2.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
